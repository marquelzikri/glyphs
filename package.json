{
  "name": "glyphs",
  "version": "0.0.1",
  "description": "REST API for glyphs",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "REST",
    "API"
  ],
  "author": "zikrimarquel",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "gravatar": "^1.8.0",
    "indonesian-news-scraper": "^2.9.1",
    "js-crawler": "^0.3.21",
    "jsonwebtoken": "^8.5.1",
    "metafetch": "^2.0.0",
    "mongoose": "^5.9.22",
    "needle": "^2.5.0",
    "news-scraper": "0.0.9",
    "open-graph-scraper": "^4.3.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.2",
    "rss-parser": "^3.8.0",
    "scrape-metadata": "^2.0.0",
    "scrappy": "^0.5.2",
    "url-metadata": "^2.4.0",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
