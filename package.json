{
  "name": "glyphs",
  "version": "0.0.1",
  "description": "REST API for glyphs",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "REST",
    "API"
  ],
  "author": "zikrimarquel",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^3.6.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "gravatar": "^1.6.0",
    "indonesian-news-scraper": "^2.9.1",
    "js-crawler": "^0.3.21",
    "jsonwebtoken": "^8.2.2",
    "metafetch": "^1.8.1",
    "mongoose": "^5.1.4",
    "needle": "^2.2.1",
    "news-scraper": "0.0.9",
    "open-graph-scraper": "^3.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "request": "^2.87.0",
    "rss-parser": "^3.4.2",
    "scrape-metadata": "^2.0.0",
    "scrappy": "^0.3.0",
    "url-metadata": "^2.1.7",
    "validator": "^10.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  }
}
